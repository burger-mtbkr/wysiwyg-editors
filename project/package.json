    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-hook-form": "^7.17.4",
    "react-quill": "^2.0.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.1",
    "typescript": "4.4.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
